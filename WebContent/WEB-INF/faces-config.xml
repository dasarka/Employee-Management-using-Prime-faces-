<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<managed-bean>
		<managed-bean-name>empMain</managed-bean-name>
		<managed-bean-class>emp.bean.EmpController</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>client</managed-bean-name>
		<managed-bean-class>emp.bean.ClientController</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>hr</managed-bean-name>
		<managed-bean-class>emp.bean.ResourceController</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>projRep</managed-bean-name>
		<managed-bean-class>emp.bean.ProjectReportController</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>emData</managed-bean-name>
		<managed-bean-class>emp.bean.HRController</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
</faces-config>
